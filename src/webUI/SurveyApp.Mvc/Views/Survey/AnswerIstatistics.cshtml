@model IEnumerable<SurveyApp.DataTransferObjects.Requests.IstatisticRequest>
@{
    ViewData["Title"] = "AnswerIstatistics";
}

<h1>Soru Analizleri</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Şık </th>
            <th scope="col">Yanıt Sayısı</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Value</td>
                <td>@item.Count</td>
            </tr>
        }

    </tbody>
</table>


